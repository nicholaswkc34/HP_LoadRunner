    <script type="text/javascript" src="js/common.js?version=8.4.0.13"></script>
    <script type="text/javascript">
        var pageLogin = {
            buttons: ['ctl00_ctl00_formBody_body_btnOKOrg', 'ctl00_ctl00_formBody_body_btnContinueIncident', 'ctl00_ctl00_formBody_body_btnLogIn'],
            btnCancelIncident: document.getElementById('ctl00_ctl00_formBody_body_btnCancelIncident'),
            txtUsername: document.getElementById('ctl00_ctl00_formBody_body_txtUsername'),
            cancelfocus: false,
            submitted: false, // if focus is on the OK button and the user presses Enter, there is one submission for the automatic button click and one for our compensator

            Load: function () {
                if (this.btnCancelIncident) {
                    this.btnCancelIncident.onfocus = function () { pageLogin.cancelfocus = true; };
                    this.btnCancelIncident.onblur = function () { pageLogin.cancelfocus = false; };
                }
                 // added by WHO
				// alert the user if they are using an old version of IE
				var version = 999; // we assume a sane browser

				if (navigator.appVersion.indexOf("MSIE") != -1) {
					version = parseFloat(navigator.appVersion.split("MSIE")[1]);
				}

				if (version < 9) {
					alert("You are using an old version of Internet Explorer. VSHOC only supports Internet Explorer 9 or higher. It also supports other browsers such as Firefox or Chrome.");
				}
				// replace WebEOC 8.2 Login with Emergency Portal Login
				document.getElementsByTagName("h1")[0].innerHTML="Emergency Portal Login";
				// hide the logo and show the WHO logo instead
				document.getElementsByClassName("poweredby")[0].style.display="none";
				var crn=document.getElementsByClassName("footer")[0].getElementsByTagName("span")[0];
				crn.innerHTML="";
				var elem = document.createElement("img");
				elem.setAttribute("src", "images/whologo.png");
				elem.setAttribute("height", "38");
				elem.setAttribute("width", "200");
				elem.setAttribute("alt", "WHO");
				crn.appendChild(elem);
				// if we're on the login page, enter credentials if they have been provided
				var un = document.getElementById("ctl00_ctl00_formBody_body_txtUsername");
				if(un)
				{			
					// are there URL parameters?
					var username = document.URL.indexOf("username=")>-1?(document.URL.substring(document.URL.indexOf("username=")+9,document.URL.substring(document.URL.indexOf("username=")+9).indexOf("&")>-1?(document.URL.indexOf("username=")+9+document.URL.substring(document.URL.indexOf("username=")+9).indexOf("&")):document.URL.length)):"";
					var pwd = document.URL.indexOf("pwd=")>-1?(document.URL.substring(document.URL.indexOf("pwd=")+4,document.URL.substring(document.URL.indexOf("pwd=")+4).indexOf("&")>-1?(document.URL.indexOf("pwd=")+4+document.URL.substring(document.URL.indexOf("pwd=")+4).indexOf("&")):document.URL.length)):"";
					var tableid =  document.URL.indexOf("tableid=")>-1?(document.URL.substring(document.URL.indexOf("tableid=")+8,document.URL.substring(document.URL.indexOf("tableid=")+8).indexOf("&")>-1?(document.URL.indexOf("tableid=")+8+document.URL.substring(document.URL.indexOf("tableid=")+8).indexOf("&")):document.URL.length)):"";
						var viewid = document.URL.indexOf("viewid=")>-1?(document.URL.substring(document.URL.indexOf("viewid=")+7,document.URL.substring(document.URL.indexOf("viewid=")+7).indexOf("&")>-1?(document.URL.indexOf("viewid=")+7+document.URL.substring(document.URL.indexOf("viewid=")+7).indexOf("&")):document.URL.length)):"";
						var boardname = document.URL.indexOf("boardname=")>-1?(document.URL.substring(document.URL.indexOf("boardname=")+10,document.URL.substring(document.URL.indexOf("boardname=")+10).indexOf("&")>-1?(document.URL.indexOf("boardname=")+10+document.URL.substring(document.URL.indexOf("boardname=")+10).indexOf("&")):document.URL.length)):"";
						var incidentid = document.URL.indexOf("incidentid=")>-1?(document.URL.substring(document.URL.indexOf("incidentid=")+11,document.URL.substring(document.URL.indexOf("incidentid=")+11).indexOf("&")>-1?(document.URL.indexOf("incidentid=")+11+document.URL.substring(document.URL.indexOf("incidentid=")+11).indexOf("&")):document.URL.length)):"";
						var positionid = document.URL.indexOf("positionid=")>-1?(document.URL.substring(document.URL.indexOf("positionid=")+11,document.URL.substring(document.URL.indexOf("positionid=")+11).indexOf("&")>-1?(document.URL.indexOf("positionid=")+11+document.URL.substring(document.URL.indexOf("positionid=")+11).indexOf("&")):document.URL.length)):"";
						var parameters = document.URL.indexOf("parameters=")>-1?(document.URL.substring(document.URL.indexOf("parameters=")+11,document.URL.substring(document.URL.indexOf("parameters=")+11).indexOf("&")>-1?(document.URL.indexOf("parameters=")+11+document.URL.substring(document.URL.indexOf("parameters=")+11).indexOf("&")):document.URL.length)):"";
						// avoid blanking out the cookies
						if(tableid!="")
							writeSessionCookie("tableid",tableid);
						if(viewid!="")
							writeSessionCookie("viewid",viewid);
						if(boardname!="")
							writeSessionCookie("caption",boardname);
						if(incidentid!="")
							writeSessionCookie("incidentid",incidentid);
						if(positionid!="")
							writeSessionCookie("positionid",positionid);
						if(parameters!="")
							writeSessionCookie("parameters",parameters);
					if(username!=""&&pwd!="")
					{
						var pd = document.getElementById("ctl00_ctl00_formBody_body_txtPassword");
						un.value=username;
						pd.value=pwd;
						
						setTimeout(function(){WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions('ctl00$ctl00$formBody$body$btnLogIn', '', true, '', '', false, false));},20000);
					}
				}
				// only do the next actions if we're on the select position / incident page
				var pdd = document.getElementById("ctl00_ctl00_formBody_body_ddlPosition");
				if(pdd)
				{				
					var idd = document.getElementById("ctl00_ctl00_formBody_body_ddlIncident");
					// now check if there is previously saved information about low bandwidth mode,  previously chosen position or incident, or preselection 
					var valCookieLowBandwidth = getLTCookie("lowbandwidth");				
					var valCookiePosition = getLTCookie("loginpositionvshoc");
					var valCookieIncident = getLTCookie("loginincidentvshoc");
					var forceIncident = getCookieValue("incidentid");
					var forcePosition = getCookieValue("positionid");															
					// if there is only one position and incident and they match the forced or cookie incident and position, submit the form
					if(pdd.options.length==1&&idd.options.length==1)
					{
						if((pdd.options[0].value==forcePosition||pdd.options[0].value==valCookiePosition)&&(idd.options[0].value==forceIncident||idd.options[0].value==valCookieIncident))
						{
							pageLogin.clickok();
							return;
						}
					}
					var initialPddIndex = pdd.selectedIndex; // this will help us know if the position was changed
					// first react to a forced position / incident
					if(forcePosition!=""||valCookiePosition!="")
					{					
						for(var i=0; i < pdd.options.length; i++){
							if(pdd.options[i].value==forcePosition&&forcePosition!="")
							{
								pdd.selectedIndex=i;
								break;
							}
							else if(pdd.options[i].value==valCookiePosition&&valCookiePosition!=""&&forcePosition=="")
							{
								pdd.selectedIndex=i;
								break;
							}
						}
					}
					// now, if it's a forced incident, select or add the value and click OK
					var incidentfound = false;
					if(forceIncident!="")
					{
						for(var i=0; i < idd.options.length; i++){
						var iival = idd.options[i].value;
						if(idd.options[i].value.indexOf(".")>-1)
							iival=idd.options[i].value.substring(idd.options[i].value.indexOf(".")+1);
							if(iival==forceIncident)
							{
								idd.selectedIndex=i;
								incidentfound=true;
								// blank out the incidentid and positionid cookies
								//writeSessionCookie("positionid","");
								writeSessionCookie("incidentid","");
								pageLogin.clickok();							
								break;							
							}
							
						}
						if(incidentfound)
								return;
							else
							{
								setTimeout(function(){__doPostBack('ctl00$ctl00$formBody$body$ddlPosition','');}, 0);
								return;
							}
					}
					// if it's a cookie incident, we select it if available
					else if(valCookieIncident!="")
					{
						for(var i=0; i < idd.options.length; i++){
							if(idd.options[i].value==valCookieIncident)
							{
								idd.selectedIndex=i;
								incidentfound=true;
								// blank out the incidentid and positionid cookies
								//writeSessionCookie("positionid","");
								writeSessionCookie("incidentid","");
								break;
							}
						}
					}
					// if we have a lowbandwidth cookie, check or uncheck lowbandwidth accordingly
					document.getElementById("checklowbandwidth").checked=(valCookieLowBandwidth=="t");
					// if we're not in an HQ position, check low bandwidth
					if((valCookieLowBandwidth==null||valCookieLowBandwidth=="")&&pdd.options[pdd.selectedIndex].text.indexOf("/")>-1&&pdd.options[pdd.selectedIndex].text.indexOf("HQ")==-1&&pdd.options[pdd.selectedIndex].text.indexOf("EURO")==-1&&pdd.options[pdd.selectedIndex].text.indexOf("AMRO")==-1)
					{
						document.getElementById("checklowbandwidth").checked=true;
						writeLTCookie("lowbandwidth", "t");
					}
					// add an onchange listener to the dropdown lists that will save a cookie when the user selects a position or incident
					if(pdd.addEventListener){
						pdd.addEventListener("change", function(e){
							pageLogin.onPositionChange();
					  }, false);
					}
					if(idd.addEventListener){
						idd.addEventListener("change", function(e){
							pageLogin.onIncidentChange();
					  }, false);
					}
					// if the position was changed and the cookie incident was not found, refresh the incident list
					if(initialPddIndex!=pdd.selectedIndex&&!incidentfound)
					{
						setTimeout(function(){__doPostBack('ctl00$ctl00$formBody$body$ddlPosition','');}, 0);
					}
				}
            },
            EnterKeySubmission: function (e) {
                if (!e) var e = window.event;
                var key = 0;
                if (Common.Key.GetCode(e) == 13 && !this.cancelfocus) { // if we're focused on a cancel button, we do not want to click OK
                    for (var i = 0; i < this.buttons.length; i++) {
                        var btn = document.getElementById(this.buttons[i]);
                        if (btn != null && this.txtUsername == null)
                            btn.click();
                    }
                }
            },
            init: function () {
                window.onload = function () { pageLogin.Load(); };
                document.forms[0].onsubmit = function () { if (pageLogin.submitted) return false; pageLogin.submitted = true; }; // make sure this submits only once
                document.body.onkeypress = function (e) { pageLogin.EnterKeySubmission(e); };
            },
			clickok: function () {
				if (!this.cancelfocus) { // if we're focused on a cancel button, we do not want to click OK
							for (var i = 0; i < this.buttons.length; i++) {
								var btn = document.getElementById(this.buttons[i]);
								if (btn != null && this.txtUsername == null)
									btn.click();
							}
						}
			},
			onPositionChange : function () {
				var pdd = document.getElementById("ctl00_ctl00_formBody_body_ddlPosition");				
				writeLTCookie("loginpositionvshoc", pdd.options[pdd.selectedIndex].value);
				var valCookieLowBandwidth = getLTCookie("lowbandwidth");
				if((valCookieLowBandwidth==null||valCookieLowBandwidth=="")&&pdd.options[pdd.selectedIndex].text.indexOf("/")>-1&&pdd.options[pdd.selectedIndex].text.indexOf("HQ")==-1&&pdd.options[pdd.selectedIndex].text.indexOf("EURO")==-1&&pdd.options[pdd.selectedIndex].text.indexOf("AMRO")==-1)
				{
					document.getElementById("checklowbandwidth").checked=true;
					writeLTCookie("lowbandwidth", "t");
				}
			},
			onIncidentChange : function () {
				var idd = document.getElementById("ctl00_ctl00_formBody_body_ddlIncident");
				writeLTCookie("loginincidentvshoc", idd.options[idd.selectedIndex].value);
			},
			bandwidthChange : function change(cbox){
                        if(cbox.checked){
                            writeLTCookie("lowbandwidth", "t");
                        } else{
                            writeLTCookie("lowbandwidth", "f");
                        }
                    }
        }
        pageLogin.init();
    </script>


