




<script type="text/javascript">
function incQuantity () {
	var q = document.getElementById('quantity');
	if (q != null) {
		var val = ++q.value;
		if (val > 12) {
			val = 12;
		}
		q.value = val;
	}
}
function decQuantity () {
	var q = document.getElementById('quantity');
	if (q != null) {
		var val = --q.value;
		if (val < 1) {
			val = 1;
		}
		q.value = val;
	}
}
</script>




